{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "types": ["node", "react"]
  },
  "include": ["pages/**/*.ts", "pages/**/*.tsx", "next-env.d.ts"]
}
